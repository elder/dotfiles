""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""

set lines=25                                  " Número de linhas ao abrir
set columns=120                               " Número de colunas ao abrir
set guioptions=-T                             " Não mostra a barra de ferramentas
set guioptions=-t                             " Não mostra os itens de menu
set guifont=Ubuntu\ Mono\ for\ Powerline\ 14  " Fonte e tamanho utilizados
set cursorline                                " Destaca a linha atual
hi cursorline guibg=#333333                   " Cor de fundo da linha destacada

